{% stylesheet %}
.mobile-unmatched{
  background-color:#fff;
  overflow:hidden;
  display:none;
}
.mobile-unmatched h2{
    color: #0b3b49;
    font-size: 20px;
    font-weight: 700;
    text-align: center;
    margin:0px;
}
.owl-prev span{
    font-size: 45px;
    color: #000;
    line-height: normal;
    display: flex;
    align-items: center;
    justify-content: center;
    transform: rotate(90deg);
    margin-top: 0px;
    margin-left: 9px;
}
.owl-prev{
   height: 60px;
    width: 50px;
   background: #b5b5b6 !important;
}
.owl-next{
    top: 128%;
    position: absolute;
    height: 60px;
    width: 50px;
    background: #b5b5b6 !important;
}
.owl-next span{
    font-size: 45px;
    color: #000;
    line-height: normal;
    display: flex;
    align-items: center;
    justify-content: center;
    transform: rotate(90deg);
    margin-top: 0px;
    margin-left: 9px;
}
.owl-nav{
    display: flex;
    justify-content: end;
    position: absolute;
    width: 100%;
    top: calc(26% - 33px);
    right: 0px;
}

@media screen and (max-width: 767px) {
 .mobile-unmatched{
  display:block;
}
.item{
  width:83%;
}
 
  }
{% endstylesheet %}

{% javascript %}
$('.owl-carousel').owlCarousel({
    loop:true,
    margin:10,
    nav:true,
    animateOut: 'fadeOut',
    responsive:{
        0:{
            items:1
        },
        600:{
            items:1
        },
        1000:{
            items:3
        }
    }
});
{% endjavascript %}

